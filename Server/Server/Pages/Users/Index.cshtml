@page
@model UserListModel
@{
    ViewData["Title"] = "Users";
}
<h1>@ViewData["Title"]</h1>
<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Users[0].Id)</th>
            <th>@Html.DisplayNameFor(model => model.Users[0].Name)</th>
            <th>@Html.DisplayNameFor(model => model.Users[0].Nhc)</th>
            <th>@Html.DisplayNameFor(model => model.Users[0].Token)</th>
            <th>@Html.DisplayNameFor(model => model.Users[0].Zip)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Users)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => user.Id)</td>
                <td>@Html.DisplayFor(modelItem => user.Name)</td>
                <td>@Html.DisplayFor(modelItem => user.Nhc)</td>
                <td>@Html.DisplayFor(modelItem => user.Token)</td>
                <td>@Html.DisplayFor(modelItem => user.Zip)</td>
                <td>
                    <a asp-page="./Edit" asp-route-id="@user.Id">Edit</a> |
                    <a asp-page="./Details" asp-route-id="@user.Id">Details</a> |
                    <a asp-page="./Delete" asp-route-id="@user.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>